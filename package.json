{
  "name": "@dylanvann/gatsby-remark-cloudinary",
  "version": "0.0.4",
  "description": "Convert media to custom components with cloudinary urls.",
  "keywords": [
    "cloudinary",
    "gatsby",
    "images"
  ],
  "license": "MIT",
  "author": "Dylan Vann <dylan@dylanvann.com>",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "lint": "tslint -p . -t codeFrame 'src/**/*.{js,jsx,ts,tsx}'",
    "build": "babel --extensions '.ts' src --out-dir . --ignore **/__tests__",
    "format": "prettier --write 'src/**/*.ts' '*.js'",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.51",
    "@dylanvann/gatsby-cloudinary": "^0.0.3",
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "cloudinary-promised": "^1.0.7",
    "is-relative-url": "^2.0.0",
    "lodash": "^4.17.4",
    "md5-file": "^4.0.0",
    "react-fast-image": "^0.0.21",
    "sharp": "^0.20.2",
    "unist-util-select": "^1.5.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.51",
    "@babel/core": "7.0.0-beta.51",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-typescript": "^7.0.0-beta.55",
    "@types/react": "^16.4.7",
    "cross-env": "^5.0.5",
    "prettier": "^1.13.7",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-config-standard": "^7.1.0",
    "typescript": "^3.0.1"
  },
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  }
}
